id: tz_secure_config
package: platform
description: >-
  This component includes the TZ secure configuration file.
category: Platform|Security|TrustZone
ui_hints:
  visibility: never
quality: beta
component_root_path: util/third_party/crypto/sl_component/sl_trustzone

provides:
  - name: tz_secure_config

requires:
  - name: trustzone_secure

include:
  - path: config/secure
    file_list:
      - path: sl_trustzone_secure_config.h

template_contribution:
  - name: component_catalog
    value: trustzone_secure_config

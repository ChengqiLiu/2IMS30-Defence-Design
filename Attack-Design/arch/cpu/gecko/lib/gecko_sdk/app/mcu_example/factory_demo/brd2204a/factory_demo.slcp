project_name: factory_demo
package: mcu
quality: production
label: MCU Example - Factory Demo (Previously known as 'helges_demo')
description: >
  This example project demonstrates a wide range of features of the EFM32GG11
  MCU and the SLSTK3701A Starter Kit.
category: Example|MCU Example
filter:
  - name: "Device Type"
    value: ["SoC"]
  - name: "MCU"
    value: ["32-bit MCU"]
  - name: "Project Difficulty"
    value: ["Advanced"]
component:
  - id: sl_system
  - id: device_init
  - id: i2cspm
    instance: [sensor]
  - id: board_control
  - id: emlib_adc
  - id: emlib_i2c
  - id: emlib_rmu
  - id: emlib_prs
  - id: glib
  - id: emlib_system
  - id: emlib_csen
  - id: si70xx_driver
  - id: emlib_gpio
  - id: emlib_timer
  - id: emlib_cryotimer
  - id: emlib_lesense
  - id: emlib_vdac
  - id: emlib_acmp
  - id: emlib_emu
  - id: emlib_lcd
  - id: si72xx_driver
  - id: emlib_common
  - id: emlib_core
  - id: emlib_rtcc
  - id: udelay
source:
  - path: app_bumode.c
  - path: app_csen.c
  - path: app_rgbled.c
  - path: app_rtcc.c
  - path: app_screens.c
  - path: app_sensor.c
  - path: main.c
include:
  - path: .
    file_list:
    - path: app_bumode.h
    - path: app_csen.h
    - path: app_rgbled.h
    - path: app_rtcc.h
    - path: app_screens.h
    - path: app_sensor.h
    - path: displaybackend.h
    - path: siliconlabs_logo.h
readme:
  - path: readme.md
ui_hints:
  highlight: readme.md
define:
  - name: "DEBUG_EFM"
configuration:
  - name: SL_BOARD_ENABLE_DISPLAY
    value: 1
  - name: SL_BOARD_ENABLE_SENSOR_RHT
    value: 1
  - name: SL_BOARD_ENABLE_SENSOR_HALL
    value: 1
tag:
  - prebuilt_demo
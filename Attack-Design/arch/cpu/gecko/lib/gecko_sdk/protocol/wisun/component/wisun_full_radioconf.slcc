    directory: rail
    condition:
    - device_sdid_84
  - path: rail/device_sdid_220/radio_settings.radioconf
    directory: rail
    condition:
    - device_sdid_220

template_contribution:
  - name: component_catalog
    value: wisun_full_radioconf

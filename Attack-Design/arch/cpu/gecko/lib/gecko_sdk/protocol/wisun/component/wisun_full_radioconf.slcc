category: "Wi-SUN"
quality: "production"
description: "Provide a radio configuration containing all Wi-SUN PHY and channels"
root_path: "protocol/wisun"

ui_hints:
  visibility: never

provides:
  - name: wisun_radioconf
  - name: wisun_full_radioconf
  - name: a_radio_config

config_file:

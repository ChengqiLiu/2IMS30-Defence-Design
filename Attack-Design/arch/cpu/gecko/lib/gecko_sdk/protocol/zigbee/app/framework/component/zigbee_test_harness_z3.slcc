  - path: protocol/zigbee/app/framework/plugin/test-harness-z3/test-harness-z3-zcl.c
include:
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3
    file_list:
      - path: test-harness-z3-core.h
      - path: test-harness-z3-mac.h

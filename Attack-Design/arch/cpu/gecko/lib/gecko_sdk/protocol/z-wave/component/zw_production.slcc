id: zw_production
label: Z-Wave Production
category: Z-Wave
description: defines and paths used for official z-wave package
package: Z-Wave
quality: production
requires:
  - name: zw_zpal_quality_mode
provides:
  - name: zw_production
  - name: zw_quality_mode
recommends:
  - id: zw_zpal_production
library:
  # Release build
  ## End devices
  ### Series 1
  - path: protocol/z-wave/ZWave/lib/libZWaveSlave_700s.a
    condition: [device_series_1, zw_end_device]
  ### Series 2
  - path: protocol/z-wave/ZWave/lib/libZWaveSlave_800s.a
    condition: [device_series_2, zw_end_device]
  ## Portable Controllers
  ### Series 1
  - path: protocol/z-wave/ZWave/lib/libZWaveControllerPortable_700s.a
    condition: [device_series_1, zw_portable_controller]
  ### Series 2
  - path: protocol/z-wave/ZWave/lib/libZWaveControllerPortable_800s.a
    condition: [device_series_2, zw_portable_controller]
  ## Controllers
  ### Series 1
  - path: protocol/z-wave/ZWave/lib/libZWaveController_700s.a
    condition: [device_series_1, zw_controller]
  ### Series 2
  - path: protocol/z-wave/ZWave/lib/libZWaveController_800s.a
    condition: [device_series_2, zw_controller]
